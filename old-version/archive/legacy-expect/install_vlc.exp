#!/usr/bin/expect -f

set timeout 300
spawn ssh -o StrictHostKeyChecking=no pi@192.168.1.103 "sudo apt-get update && sudo apt-get install -y vlc"

expect {
    "password:" {
        send "raspberry\r"
        exp_continue
    }
    "\[sudo\] password for pi:" {
        send "raspberry\r"
        exp_continue
    }
    eof
}